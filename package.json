{
  "name": "bb-qol",
  "version": "0.0.1",
  "description": "Chrome Extension that adds Quality of Life features to Blackboard",
  "scripts": {
    "build": "npm run build:eventPage && npm run build:pageTree && npm run copy:statics",
    "clean": "rm -rf build/ && mkdir build",
    "watch:eventPage": "watchify -o build/eventPages.js -t [ babelify --presets [ es2015 ] ] -v -d src/eventPages/index.js",
    "watch:pageTree": "watchify -o build/buildPageTree.js -t [ babelify --presets [ es2015 ] ] -v -d src/contentScripts/buildPageTree.js",
    "build:eventPage": "browserify src/eventPages/index.js -t [ babelify --presets [ es2015 ] ] -o build/eventPages.js",
    "build:pageTree": "browserify src/contentScripts/buildPageTree.js -t [ babelify --presets [ es2015 ] ] -o build/buildPageTree.js",
    "copy:statics": "cp manifest.json build/manifest.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redice44/bb-qol.git"
  },
  "author": "Matt Thomson <red.cataclysm@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/redice44/bb-qol/issues"
  },
  "homepage": "https://github.com/redice44/bb-qol#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "uglify": "^0.1.5",
    "watchify": "^3.7.0"
  }
}
